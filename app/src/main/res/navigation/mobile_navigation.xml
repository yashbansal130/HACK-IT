<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.hack_it.ui.home.HomeFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.hack_it.ui.Studio.StudioFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_studio" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.hack_it.ui.Profile.ProfileFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_profile" />
</navigation>